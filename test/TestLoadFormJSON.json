{
	"form-name"	: "data-wisata-from-json",
	
	"form-fields": {
		"id" : {
			"type" : "FORM_HIDDEN"
		},
		"csrf" : {
			"type" : "FORM_HIDDEN"
		},
		"name" : {
			"type" : "FORM_STRING",
			"length" : 100,
			"caption" : "Name"
		},
		"province" : {
			"type" : "FORM_TEXT",
			"caption" : "Provinsi"
		},
		"village" : {
			"type" : "FORM_TEXT",
			"caption" : "Village"
		}
	},

	"form-buttons" : {
		"submit:OnFormInsert" :{
			"caption" : "Tambah Data",
			"type" : "submit",
			"style" : ".btn-submit" 
		},
		"submit:OnFormUpdate" :{
			"caption" : "Simpan Perubahan",
			"type" : "submit" 
		},
		"cancel" : {
			"caption" : "Batalkan",
			"style" : ".btn-submit", 
			"type" : "button",
			"action":"OnClick:'window.history.back();'"
		}
	}
}